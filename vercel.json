{
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      },
      {
        "src": "server/package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "server/routes/(.*)",
        "dest": "server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/$1"
      }
    ]
  }
  